	<queryString>
		<![CDATA[select 
p.merk_produk namaBarang,
p.id kodeBarang,
o.quantity quantity,
p.harga hargaSatuan,
o.total_harga subHarga,
t.alamat_toko alamatToko,
sj.tgl_kirim tanggalKirim
from orderan o, surat_jalan sj, toko t, produk p
where o.toko = t.id 
