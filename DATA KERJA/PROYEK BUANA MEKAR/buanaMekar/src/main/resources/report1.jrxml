	<queryString>
		<![CDATA[SELECT
     t.`nama_toko` AS namaToko,
     sj.`tgl_kirim` AS tanggalKirim,
     p.`id` AS kodeBarang,
     p.`merk_produk` AS namaBarang,
     o.`quantity` AS quantity,
     p.`harga` AS hargaSatuan,
     o.`total_harga` AS subHarga,
     i.`ppn` AS ppn,
     i.`total_harga` AS totalHarga
FROM
     `orderan` o INNER JOIN `toko` t ON o.`toko` = t.`id`
     INNER JOIN `surat_jalan` sj ON o.`id` = sj.`orderan`
     INNER JOIN `produk` p ON o.`produk` = p.`id`
     INNER JOIN `invoice` i ON sj.`id` = i.`surat_jalan`
WHERE
     t.nama_toko LIKE 'ALIONG COMPANY'
 AND sj.tgl_kirim LIKE '18/08/2020 23:04:55']]>
	</queryString>
